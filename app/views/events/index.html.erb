<h1>Meine Events</h1>

<table>
  <thead>
    <tr>
      <th>Titel</th>
      <th>Countdown</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= yearcount event.happend_at%></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<nav>
<%= link_to "Startseite", '/' %> |
<%= link_to 'New Event', new_event_path %> |
<%= link_to "Freunde Einladen", '/users/edit' %>
</nav>


